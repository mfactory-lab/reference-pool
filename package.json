      "uuidv4": "*"
    },
    "overrides": {
      "@eslint/plugin-kit": ">=0.2.3",
      "@solana/web3.js": ">=1.98.0",
      "@trezor/connect-web": ">=9.4.4",
      "@wagmi/core": ">=2.15.1",
      "axios": ">=1.7.4",
      "bs58": ">=6.0.0",
      "cross-spawn": ">=7.0.5",
      "elliptic": ">=6.6.0",
      "glob": ">=11.0.0",
      "minimatch": "9.0.3",
      "react": ">=18.0.0",
      "react-dom": ">=18.0.0",
      "rollup": ">=4.22.4",
      "secp256k1": ">=5.0.1",
      "ws": ">=8.17.1"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm run lint:fix"
  }
}
