{
  "name": "rerference-pool",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "nuxt dev",
    "build": "cross-env NODE_ENV=production nuxt generate",
    "build:ssg": "cross-env NODE_ENV=production nuxt generate",
    "build:ssr": "cross-env NODE_ENV=production nuxt build",
    "build:staging:ssg": "cross-env NODE_ENV=staging nuxt generate",
    "preview": "nuxt preview",
    "lint": "oxlint && eslint",
    "lint:fix": "oxlint --fix && eslint --fix",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "ross-env  npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.6",
    "@gtm-support/vue-gtm": "^3.1.0",
    "@pinia/nuxt": "^0.11.1",
    "@solana/spl-stake-pool": "github:mfactory-lab/spl-stake-pool#d7b4b34",
    "@solana/spl-token": "^0.4.13",
    "@solana/wallet-adapter-vue": "github:mfactory-lab/solana-wallet-adapter-vue",
    "@solana/web3.js": "^1.98.2",
    "@vueuse/nuxt": "^13.0.0",
    "axios": "^1.7.9",
    "bn.js": "^5.2.2",
    "buffer": "^6.0.3",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nuxt": "^3.17.5",
    "pinia": "^3.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "swiper": "^11.2.1",
    "unplugin-imagemin": "^0.5.20",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1",
    "wallets": "workspace:^"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@bootstrap-vue-next/nuxt": "^0.28.3",
    "@nuxt/types": "^2.18.1",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.14",
    "@vite-pwa/nuxt": "^0.10.6",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vueuse/core": "^10.11.1",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.26.20",
    "critters": "^0.0.24",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^0.1.3",
    "eslint-plugin-oxlint": "^0.4.0",
    "lint-staged": "^15.4.1",
    "oxlint": "^0.4.4",
    "prettier": "^3.4.2",
    "sass": "1.77.6",
    "simple-git-hooks": "^2.11.1",
    "typescript": "5.4.5",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^5.4.11",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vue-tsc": "^2.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "tslib"
      ]
    },
    "allowedDeprecatedVersions": {
      "@solana/wallet-adapter-exodus": "*",
      "@solana/wallet-adapter-magiceden": "*",
